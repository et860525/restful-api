{ "compilerOptions": 
  {
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "rootDir": "./src",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
	  "outDir": "./dist"
  },
  "include": ["src/**/*.ts"], 
  "exclude": ["node_modules", "dist"]
}
